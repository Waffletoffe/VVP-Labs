#include <iostream>
using namespace std;


int main()
{

	int day = 0, mont = 0;
	cout<<"Введите месяц:\n";
	cin>>mont;
	cout<<"Введите день:\n";
	cin>>day;

	if (day == 1) {
		cout<<"Первое ";
	}
	else  if (day == 2) {
		cout<<"Второе  ";
	}
	if (day == 3) {
		cout<<"Третье  ";
	}
	else  if (day == 4) {
		cout<<"Четвертое  ";
	}
	if (day == 5) {
		cout<<"Пятое  ";
	}
	else  if (day == 6) {
		cout<<"Шестое  ";
	}
	if (day == 7) {
		cout<<"Седьмое  ";
	}
	else  if (day == 8) {
		cout<<"Восьмое  ";
	}
	if (day == 9) {
		cout<<"Девятое  ";
	}
	else  if (day == 10) {
		cout<<"Десятое  ";
	}
	if (day == 11) {
		cout<<"Одиннадцатое  ";
	}
	else  if (day == 12) {
		cout<<"Двенадцатое  ";
	}
	if (day == 13) {
		cout<<"Тринадцатое  ";
	}
	else  if (day == 14) {
		cout<<"Четарнадцатое  ";
	}
	if (day == 15) {
		cout<<"Пятнадцатое  ";
	}
	else  if (day == 16) {
		cout<<"Шестандцатое  ";
	}
	if (day == 17) {
		cout<<"Семнадцатое  ";
	}
	else  if (day == 18) {
		cout<<"Восемнадцатое  ";
	}
	if (day == 19) {
		cout<<"Девятнадцатое  ";
	}
	else  if (day == 20) {
		cout<<"Двадцатое  ";
	}
	if (day == 21) {
		cout<<"Двадцать Первое  ";
	}
	else  if (day == 22) {
		cout<<" Двадцать Второе  ";
	}
	if (day == 23) {
		cout<<"Двадцать третье  ";
	}
	else  if (day == 24) {
		cout<<"Двадцать четвертое  ";
	}
	if (day == 25) {
		cout<<"Двадцать пятое  ";
	}
	else  if (day == 26) {
		cout<<"Двадцать шестое  ";
	}
	if (day == 27) {
		cout<<"Двадцать седьмое  ";
	}
	else  if (day == 28) {
		cout<<"Двадцать восьмое  ";
	}
	if (day == 29) {
		cout<<"Двадцать девятое  ";
	}
	else  if (day == 30) {
		cout<<"Тридцатое  ";
	}
	if (day == 31) {
		cout<<" Тридцать первое  ";
	}
	if (mont == 1) {
		cout<<"января";
	}
	if (mont == 2) {
		cout<<"февраля";
	}
	if (mont == 3) {
		cout<<"марта";
	}
	if (mont == 4) {
		cout<<"апреля";
	}
	if (mont == 5) {
		cout<<"мая";
	}
	if (mont == 6) {
		cout<<"июня";
	}
	if (mont == 7) {
		cout<<"июля";
	}
	if (mont == 8) {
		cout<<"августа";
	}
	if (mont == 9) {
		cout<<"сентября";
	}
	if (mont == 10) {
		cout<<"октября";
	}
	if (mont == 11) {
		cout<<"ноября";
	}
	if (mont == 12) {
		cout<<"декабря";
	}

	return 0;

}
